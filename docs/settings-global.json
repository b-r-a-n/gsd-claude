{
  "permissions": {
    "allow": [
      "Bash(~/.claude/commands/gsd/**)",
      "Bash(git status*)",
      "Bash(git add*)",
      "Bash(git commit*)",
      "Bash(git log*)",
      "Bash(git diff*)",
      "Bash(git rev-parse*)",
      "Bash(git config*)",
      "Bash(mkdir -p ~/.claude/planning*)",
      "Read(~/.claude/planning/**)",
      "Write(~/.claude/planning/**)",
      "Read(/tmp/gsd-*)",
      "Write(/tmp/gsd-*)"
    ],
    "deny": []
  }
}
